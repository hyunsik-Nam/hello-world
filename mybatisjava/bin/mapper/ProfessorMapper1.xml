<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="professor">
	<select id="count" resultType="int">
		select count(*) from professor
	</select>
	
	<select id="info" resultType="Professor">
		select * from professor
	</select>
	
	<select id="info101" resultType="Professor" parameterType="int">
		select * from professor where deptno=#{value}
	</select>
	
	<select id="kim" resultType="Professor" parameterType="map">
		select * from professor where position=#{position} and name like '${name}%'
	</select>
</mapper>